# Environment Variables Template
# Copy this file to .env and fill in the actual values

# Next.js Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret-here

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/seezee

# AI Services (REQUIRED for AI features)
ANTHROPIC_API_KEY=sk-ant-your-key-here
OPENAI_API_KEY=sk-proj-your-key-here

# Google Maps (Optional - for geocoding leads)
GOOGLE_MAPS_API_KEY=your-google-maps-key

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# Email Configuration (Resend)
RESEND_API_KEY=re_your-resend-api-key

# File Upload Configuration (UploadThing)
UPLOADTHING_TOKEN=your-uploadthing-token

# GitHub Integration (Optional - for commit feed events)
GITHUB_TOKEN=ghp_your-github-personal-access-token
GITHUB_REPO_SLUG=owner/repository-name

# Feature Flags
SEEZEE_FEATURE_FEED=true
SEEZEE_FEATURE_MESSAGES=true
SEEZEE_FEATURE_INVOICES=true
SEEZEE_FEATURE_BILLING=true